get:
  description: Get POI limit
  queryParameters:
        msisdn:
          type: number
          required: true
          description: The MSISDN of the customer
        poi_number:
          type: number
          required: true
          description: POI Number
  responses: 
    200:
      description: On Successful response.  
      body: 
        application/json:
          example: !include         examples/wmt-mfs-crm-poi-limit.json
    304:
      description: On unchanged response.